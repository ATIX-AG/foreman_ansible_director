{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["dom", "dom.iterable"],
    "skipLibCheck": true,
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "paths": {"foremanReact/*" :  ["./types/foremanReact/*"]},
    "types": ["node", "jest", "@testing-library/jest-dom"],
  },
  "include": [
    "./webpack/**/*"
  ],
}
